{
  "sourceMaps": "inline",
  "retainLines": true,
  "presets": [
    "stage-0",
    [
      "env",
      {
        "targets": {
          "node": "6.0.0",
          "browsers": [
            "last 2 versions"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "lodash"
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-runtime",
        "istanbul"
      ]
    }
  }
}